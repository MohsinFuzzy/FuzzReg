flr<-function (Y,X,type){
if (type==1){
lm1<-lm(Y[,1]~X[,1])
lm2<-lm(Y[,2]~X[,2])
lm3<-lm(Y[,3]~X[,3])
}
if (type==2){
lm1<-lm(Y[,1]~X)
lm2<-lm(Y[,2]~X)
lm3<-lm(Y[,3]~X)
}
coef<-cbind(lm1$coefficients,lm2$coefficients,lm3$coefficients)
dimnames(coef)=list(c("Intercept","Slope"),c("Left","Centre","Right"))
return(list(c("Fuzzy Linear Regression"),coefficients=coef))
}
aov<-function (Y,X,type){
if (type==1){
lm1<-lm(Y[,1]~X[,1])
lm2<-lm(Y[,2]~X[,2])
lm3<-lm(Y[,3]~X[,3])
}
if (type==2){
lm1<-lm(Y[,1]~X)
lm2<-lm(Y[,2]~X)
lm3<-lm(Y[,3]~X)
}
coef<-cbind(aov(lm1),aov(lm2),aov(lm3))
return(list(c("Fuzzy Linear Regression"),anova=coef))
}
aov(Y,X)
aov(Y,X,1)
aov
