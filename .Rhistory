anov<-function (Y,X,type){
if (type==1){
lm1<-lm(Y[,1]~X[,1])
lm2<-lm(Y[,2]~X[,2])
lm3<-lm(Y[,3]~X[,3])
}
if (type==2){
lm1<-lm(Y[,1]~X)
lm2<-lm(Y[,2]~X)
lm3<-lm(Y[,3]~X)
}
coef<-cbind(aov(lm1),aov(lm2),aov(lm3))
return(list(c("Fuzzy Linear Regression"),anova=coef))
}
anov(Y,X,1)
sumry<-function (Y,X,type){
if (type==1){
lm1<-lm(Y[,1]~X[,1])
lm2<-lm(Y[,2]~X[,2])
lm3<-lm(Y[,3]~X[,3])
}
if (type==2){
lm1<-lm(Y[,1]~X)
lm2<-lm(Y[,2]~X)
lm3<-lm(Y[,3]~X)
}
coef<-rbind(summary(lm1),summary(lm2),summary(lm3))
return(list(c("Fuzzy Linear Regression"),sum=coef))
}
summary(Y,X,1)
sumry(Y,X,1)
